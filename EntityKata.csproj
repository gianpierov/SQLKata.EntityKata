<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>net6.0</TargetFramework>
        <ImplicitUsings>enable</ImplicitUsings>
        <Nullable>enable</Nullable>
        <PackageId>Entitykata_Gianpiero_Vecchi</PackageId>
        <Version>1.0.0</Version>
        <Authors>Gianpiero Vecchi</Authors>
        <Description>Entity Manager using SQLKata</Description>
        <PackageProjectUrl>www.gianpierovecchi.com</PackageProjectUrl>
        <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
	    <PackageLicenseExpression>MIT OR Apache-2.0</PackageLicenseExpression>
    </PropertyGroup>

    <ItemGroup>
      <PackageReference Include="SqlKata" Version="2.3.7" />
      <PackageReference Include="SqlKata.Execution" Version="2.3.7" />
    </ItemGroup>

</Project>
